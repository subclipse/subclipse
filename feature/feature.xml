<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.tigris.subversion.subclipse"
      label="Subclipse"
      version="1.2.0"
      provider-name="tigris.org"
      plugin="org.tigris.subversion.subclipse.core"
      colocation-affinity="org.tigris.subversion.subclipse">
   <install-handler/>

   <description url="%changesURL">
      %description
   </description>

   <copyright url="http://subclipse.tigris.org/">
      http://subclipse.tigris.org/
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://subclipse.tigris.org/update_1.2.x"/>
      <discovery label="%updateSiteName" url="http://subclipse.tigris.org/update_1.2.x"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.resources" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.team.core" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.team.ui"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.eclipse.help"/>
   </requires>

   <plugin
         id="org.tigris.subversion.javahl.win32"
         os="win32"
         download-size="0"
         install-size="0"
         version="1.2.0"
         fragment="true"/>

   <plugin
         id="org.tigris.subversion.subclipse.core"
         download-size="0"
         install-size="0"
         version="1.2.0"
         unpack="false"/>

   <plugin
         id="org.tigris.subversion.subclipse.ui"
         download-size="0"
         install-size="0"
         version="1.2.0"
         unpack="false"/>

   <plugin
         id="org.tigris.subversion.subclipse.doc"
         download-size="0"
         install-size="0"
         version="1.2.0"
         unpack="false"/>

</feature>
